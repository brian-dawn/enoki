
# We can emit regular JS with backticks.
# Code comments don't exist yet lawl... but this works.


inc = +(1)
result = inc(4)
println(result)

# Lists are immutable.
some-list = [1, 2, 3]
another-list = map(inc, push(some-list, 4))
println(some-list)
println(another-list)


println(reduce(+, another-list))
